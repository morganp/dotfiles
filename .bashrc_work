#Work Specific .bashrc Settings
. /res/modules/init/bash
module load wolfson


# Set this to your preferred printer.
PRINTER=2prm14500

#Enable DC queueing
export SNPSLMD_QUEUE=true

export NS_BITMODE=64

alias exit='history -a && exit'

###############################
## Specific Paths
###############################
export PATH=~/bin/my_scripts/:$PATH
export PATH=~/bin/tkdiff-4.1.4-unix/:$PATH

export PATH=~/bin/apache-ant-1.7.1/bin:$PATH

export PATH=/user/eng/morgan/bin:$PATH
export PATH=/home/morgan/.gem/ruby/1.8/bin:$PATH

###############################
## Fix DISPLAY when reconnecting to screen
###############################
if [ -f ~/.realdisplay ];
then
   source ~/.realdisplay
fi

###############################
## Specific alias
###############################
alias goto_j='cd /user/eng/morgan/'

goto_j
