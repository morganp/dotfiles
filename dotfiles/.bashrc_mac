#Bashrc for MAC

#User Scripts
post_add_path ~/bin/scripts_generic

#Scheme
post_add_path ~/bin/racket/bin

#Manual nginx install
post_add_path ~/bin/nginx/sbin

#Add path for mysql package
post_add_path /usr/local/mysql/bin/

#Add Path for google code install of git (Source control)
post_add_path /usr/local/git/bin/


################################
## Custom Alias
################################

alias nginx="nginx -p ~/bin/nginx/ -s stop && nginx -p ~/bin/nginx/ "
alias tkdiff="/Applications/TkDiff.app/Contents/MacOS/Wish\ Shell"
