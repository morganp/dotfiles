<?xml version="1.0" encoding="utf-8"?>
<CustomKeySet derivedfrom="Emacs" modifieddefault="true">
   <Context id="Global">
      <Action id="page-down">
         <Stroke code="VK_PAGE_DOWN"/>
      </Action>
      <Action id="comment">
         <Stroke code="VK_R" ctrl="on"/>
      </Action>
      <Action id="undo">
         <Stroke code="VK_Z" ctrl="on"/>
      </Action>
      <Action id="inc-search-backward"/>
      <Action id="split-screen-top-bottom"/>
      <Action id="save">
         <Stroke code="VK_S" ctrl="on"/>
      </Action>
      <Action id="save-as"/>
      <Action id="save-workspace"/>
      <Action id="inc-search-forward"/>
      <Action id="split-screen-left-right"/>
      <Action id="cut-to-clipboard">
         <Stroke code="VK_X" ctrl="on"/>
      </Action>
      <Action id="split-screen-switch-focus">
         <Stroke code="VK_F6"/>
      </Action>
      <Action id="select-all"/>
      <Action id="caret-begin-line">
         <Stroke code="VK_HOME"/>
      </Action>
      <Action id="open-new"/>
      <Action id="copy-to-clipboard">
         <Stroke code="VK_C" ctrl="on"/>
      </Action>
      <Action id="close-item"/>
      <Action id="paste-from-clipboard">
         <Stroke code="VK_V" ctrl="on"/>
      </Action>
      <Action id="split-screen-off"/>
      <Action id="save-all"/>
   </Context>
   <Context id="SpreadsheetTable">
      <Action id="select-all">
         <Stroke code="VK_A" ctrl="on"/>
      </Action>
   </Context>
</CustomKeySet>